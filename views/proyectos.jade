extends layout

block content
  .container
      h1 Proyectos
      .bs-docs-example
       table.table.table-bordered
        thead
          tr
            th Ver proyecto
            th Sumario
            th Fecha
        tbody
        each proyecto in proyectos.payload.proyectos
          tr
            td
             a(href="/proyecto/"+proyecto.proyecto.uuid)
              | Link
            td
             | #{ proyecto.proyecto.sumario}
            td
             | #{ proyecto.proyecto.publicacion_fecha}