extends layout

block content
  .container
      h1 Proyectos
      .bs-docs-example
       table.table.table-bordered
        thead
          tr
            th Fecha
            th Ver proyecto
            th Sumario
        tbody
          each proyecto in proyectos.payload.proyectos
            tr
              td #{ proyecto.proyecto.publicacion_fecha}
              td 
                a(href="/proyecto/"+proyecto.proyecto.uuid) Link
              td #{ proyecto.proyecto.sumario}

      .pagination
       ul
        li
         a(href='#') Prev
        each link in proyectos.links
          li
            a(href='#') 
        li
          a(href='') Next

